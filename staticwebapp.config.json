{
    "routes": [
      {
        "route": "/profile*",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/additional*",
        "allowedRoles": ["admin"]
      }
    ],
    "responseOverrides": {
      "401": {
        "statusCode": 302,
        "redirect": "/.auth/login/aad"
      }
    }
  }